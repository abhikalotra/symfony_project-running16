{% include '::header.html.twig' %}





<div class="wrapper row3">
  <main class="container clear"> 
    <!-- main body -->
    <p class="heading"> 
      {% if app.session.get('sessionname')   %}  
    
			<font color = "green" >	* {{app.session.get('sessionname') }} </font>						
     
     {% else %}
				<font color = "#E2494B" >!! Not login  Please Login </font>		
     	{% endif %}</p>
     	
    <h2 class="heading uppercase bold font-x2"> Questions By  Tags
    
     
     
    </h2>
    

    <div class="group products">
	   
	 
	 
	     {{ asset('app_dev.php/show_post_by_tags/') }}
	   
	    
	     
